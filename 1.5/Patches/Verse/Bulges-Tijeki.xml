<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/AlienRace.ThingDef_AlienRace[defName="Comp_Tijeki"]/alienRace/generalSettings/alienPartGenerator/bodyAddons</xpath>
		<value>
			<li>
				<name>Belly</name>
				<userCustomizable>false</userCustomizable>
				<allowColorOverride>false</allowColorOverride>
				<path>Belly/Empty</path>
				<inFrontOfBody>true</inFrontOfBody>
				<colorChannel>skin</colorChannel>
				<scaleWithPawnDrawsize>true</scaleWithPawnDrawsize>
				<conditions>
					<Posture>
						<drawnInBed>false</drawnInBed>
					</Posture>
					<RotStage>Fresh</RotStage>
				</conditions>
				<extendedGraphics>
					<Hediff For="RV2B_BellyBulge">
						<path>Belly/Empty</path>
						<extendedGraphics>
							<Severity For="0.4">
								<BodyType For="Male">Belly/Base/Belly_Male</BodyType>
								<BodyType For="Female">Belly/Base/Belly_Female</BodyType>
								<BodyType For="Thin">Belly/Base/Belly_Thin</BodyType>
							</Severity>
						</extendedGraphics>
					</Hediff>
				</extendedGraphics>
			</li>
		</value>
	</Operation>
</Patch>